# Secret key for JSON Web Token.
# Example: a-string-secret-at-least-256-bits-long
JWT_SECRET=

# Super admin's token, used to create platform admins.
# Example: i-am-a-super-admin
SUPER_ADMIN_TOKEN=

# PostgreSQL database connection string.
# Example: postgres://username:password@postgres:5432/database
POSTGRES_URL=

# List of Kafka brokers, separated by commas.
# Example: localhost:9092,localhost:9093
KAFKA_BROKERS=

# Google OAuth 2.0 client ID for web.
# Example: xxxxxxxxxxxx-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx.apps.googleusercontent.com
GOOGLE_WEB_CLIENT_ID=

# Google OAuth 2.0 client ID for Android.
# Example: xxxxxxxxxxxx-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx.apps.googleusercontent.com
GOOGLE_ANDROID_CLIENT_ID=

# Google OAuth 2.0 client ID for iOS.
# Example: xxxxxxxxxxxx-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx.apps.googleusercontent.com
GOOGLE_IOS_CLIENT_ID=
